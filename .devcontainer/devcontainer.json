{
	"name": "go",
	"dockerComposeFile": [
		"./docker-compose.yml"
	],
	"service": "app",
	"workspaceFolder": "/go/src/app",
	"settings": {
		"terminal.integrated.shell.linux": "/bin/ash",
		"go.toolsManagement.checkForUpdates": "local",
		"go.gopath": "/go",
		"go.useLanguageServer": true
	},
	"extensions": [
		"ms-vscode.go",
		"golang.go",
		"humao.rest-client"
	],
	"shutdownAction": "stopCompose"
}